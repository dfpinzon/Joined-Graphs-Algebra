Cell[TextData[{
 "Recall that for a graph ",
 Cell[BoxData[
  FormBox["G", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "10e34752-4877-49c0-909b-8b81f32153e6"],
 " with ",
 Cell[BoxData[
  FormBox["m", TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "ed767258-8fcd-47a1-a7b0-1c3a1cf86f13"],
 " vertices, for ",
 Cell[BoxData[
  FormBox[
   RowBox[{"j", "=", "1"}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "83270e22-d908-4414-883d-d23bcc8ccc4a"],
 "\n",
 Cell[BoxData[
  FormBox[
   RowBox[{
    RowBox[{
     SubscriptBox["\[Phi]", "1"], "(", "G", ")"}], "=", 
    RowBox[{"(", GridBox[{
       {
        RowBox[{"|", "G", "|"}], 
        RowBox[{"|", 
         RowBox[{"G", "\\", "m"}], "|"}]},
       {
        RowBox[{"|", 
         RowBox[{"G", "\\1"}], "|"}], 
        RowBox[{"|", 
         RowBox[{"G", "\\", 
          RowBox[{"{", 
           RowBox[{"1", ",", "m"}], "}"}]}], "|"}]}
      }], ")"}]}], TraditionalForm]],
  FormatType->TraditionalForm,ExpressionUUID->
  "45cace71-b7f8-425d-a481-21fe33dda812"]
}], "Text",
 CellChangeTimes->{{3.8722497922435703`*^9, 3.87224987036135*^9}, {
